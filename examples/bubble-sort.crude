define bubbleSort(arr) {
  for(i, 0, length(arr)) {
    for(j, 0, length(arr)) {
      let prevVal = arr[j];
      let nextVal = arr[j + 1];
      if(prevVal > nextVal) {
        let arr = change(arr, j, nextVal);
        change(arr, j + 1, prevVal);
      }
    }
  }
  return arr;
}
